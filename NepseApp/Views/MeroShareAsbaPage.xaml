<UserControl x:Class="NepseApp.Views.MeroShareAsbaPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"    
             xmlns:views="clr-namespace:NepseApp.Views"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <TabControl Style="{DynamicResource MaterialDesignTabControl}" 
                SelectedItem="{Binding SelectedItem}">
        <TabItem Header="Application Report">
            <views:MeroShareAsbaApplicationReportPage />
        </TabItem>
        <TabItem Header="Old Application Report" >
            <views:MeroShareAsbaOldApplicationReportPage />
        </TabItem>
    </TabControl>
</UserControl>
